  <!--build:js /js/vendor-boxed.min.js -->
  <script src="/plugins/jQuery/jQuery-2.1.4.min.js"></script>
  <script src="/plugins/bootstrap/js/bootstrap.min.js"></script>
  <script src="/plugins/slimScroll/jquery.slimscroll.min.js"></script>
  <script src="/js/custom/offcanvas.js"></script>
  <script src="/js/custom/box.js"></script>
  <script src="/js/custom/tree-menu.js"></script>
  <!-- endbuild -->

  {% block customScripts %} {% endblock %}

  <!--build:js /js/main-boxed.min.js -->
  <script src="/js/main.js"></script>
  <!-- endbuild -->


