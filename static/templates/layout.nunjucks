<html itemscope itemtype="https://schema.org/" lang="en-US">
<!--<![endif]-->

<head>

  <meta charset="utf-8">


  <title>Atlas Admin</title>

  {% include "partials/header-meta.nunjucks" %}

  <!-- Font awesome -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">


  <link href='https://fonts.googleapis.com/css?family=Work+Sans:400,600' rel='stylesheet' type='text/css'>

  {% block customStyles %} {% endblock %}


  <!--build:css /css/styles.min.css id="main-stylesheet"-->
  <link rel="stylesheet" href="/plugins/aos/aos.css">
  <link rel="stylesheet" id="main-stylesheet" href="/css/styles.css">
  <!--endbuild-->

  <!--build:js /js/main.min.js -->
  <script src="/plugins/jQuery/jQuery-2.1.4.min.js"></script>
  <script src="/plugins/bootstrap/js/bootstrap.min.js"></script>
  <script src="/plugins/slimScroll/jquery.slimscroll.min.js"></script>
  <script src="/plugins/aos/aos.js"></script>
  <script src="/plugins/pjax/jquery.pjax.min.js"></script>
  <script src="/js/custom/pjax-setup.js"></script>

  <script src="/js/custom/offcanvas.js"></script>
  <script src="/js/custom/box.js"></script>
  <script src="/js/custom/tree-menu.js"></script>
  <script src="/js/main.js"></script>
  <!-- endbuild -->

</head>

<body class="skin-deep-blue sidebar-image misty-mountains">

  <div class="wrapper">
    {% include "partials/header.nunjucks" %}
    {% include "partials/sidebar.nunjucks" %}


    <div class="page-loader">
      <div class="preloader-lg"></div>
    </div>

    <div class="content-wrapper-transition">
      <div class="content-wrapper-inner">
        {% block content %} {% endblock %}
        {% include "partials/footer.nunjucks" %}

        {% block customScripts %} {% endblock %}
      </div>
    </div>

    {% include "partials/customization-modal.nunjucks" %}

  </div>

</body>

</html>
